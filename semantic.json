{
    "base": "semantic/",
    "paths": {
        "source": {
            "config": "src/theme.config",
            "definitions": "src/definitions/",
            "site": "src/site/",
            "themes": "src/themes/"
        },
        "output": {
            "packaged": "dist/",
            "uncompressed": "dist/components/",
            "compressed": "dist/components/",
            "themes": "dist/themes/"
        },
        "clean": "dist/"
    },
    "permission": false,
    "autoInstall": true,
    "rtl": false,
